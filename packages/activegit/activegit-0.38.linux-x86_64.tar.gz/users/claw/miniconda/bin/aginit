#!/users/claw/miniconda/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'activegit==0.38','console_scripts','aginit'
__requires__ = 'activegit==0.38'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('activegit==0.38', 'console_scripts', 'aginit')()
    )
