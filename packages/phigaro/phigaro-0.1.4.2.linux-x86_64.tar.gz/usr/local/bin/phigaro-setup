#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'phigaro==0.1.4.2','console_scripts','phigaro-setup'
__requires__ = 'phigaro==0.1.4.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('phigaro==0.1.4.2', 'console_scripts', 'phigaro-setup')()
    )
