{"ETOPO": {
    "rel_path": "etopo",
    "vars": {
        "topography": {
            "default_resolution": "1min",
            "5min": {
                "default_tscale": "stationary",
		"varnames": {
                    "latitude": "lat",
                    "longitude": "lon",
                    "height": "z"
                    },
                "stationary": [
                    {
                        "filename": "etopo5.nc",
                        "url": "http://oos.soest.hawaii.edu/thredds/ncss/etopo5?var=ROSE&disableLLSubset=on&disableProjSubset=on&horizStride=1&addLatLon=true"
                        }
                    ]
                },
            "1min": {
                "default_tscale": "stationary",
                "varnames": {
                    "latitude": "lat",
                    "longitude": "lon",
                    "height": "z"
                    },
                "stationary": [
                    {
                        "filename": "etopo1.nc",
                        "url": "http://www.ngdc.noaa.gov/thredds/fileServer/relief/ETOPO1/thredds/ETOPO1_Bed_g_gmt4.nc"
                        }
                    ]
                }
            }
        }
    }
}
