#!/home/szczepan/.virtualenvs/pyh/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pyhistory==2.1','console_scripts','pyhistory'
__requires__ = 'pyhistory==2.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pyhistory==2.1', 'console_scripts', 'pyhistory')()
    )
