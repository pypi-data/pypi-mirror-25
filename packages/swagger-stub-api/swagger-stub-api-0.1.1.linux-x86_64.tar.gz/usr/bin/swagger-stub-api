#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'swagger-stub-api==0.1.1','console_scripts','swagger-stub-api'
__requires__ = 'swagger-stub-api==0.1.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('swagger-stub-api==0.1.1', 'console_scripts', 'swagger-stub-api')()
    )
