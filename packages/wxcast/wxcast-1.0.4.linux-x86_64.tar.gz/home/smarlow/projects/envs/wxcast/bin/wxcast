#!/home/smarlow/projects/envs/wxcast/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'wxcast==1.0.4','console_scripts','wxcast'
__requires__ = 'wxcast==1.0.4'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('wxcast==1.0.4', 'console_scripts', 'wxcast')()
    )
