[Unit]
Description={{journald_cloudwatch_service}}
Wants=basic.target
After=basic.target network.target

[Service]
User=root
Group=root
ExecStart={{go_bin}}/{{journald_cloudwatch_service}} /etc/journald-cloudwatch.conf
KillMode=process
Restart=on-failure
RestartSec=42s

[Install]
WantedBy=multi-user.target