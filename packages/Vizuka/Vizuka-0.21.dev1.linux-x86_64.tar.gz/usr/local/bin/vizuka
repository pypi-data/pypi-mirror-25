#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'Vizuka==0.21.dev1','console_scripts','vizuka'
__requires__ = 'Vizuka==0.21.dev1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Vizuka==0.21.dev1', 'console_scripts', 'vizuka')()
    )
